{
    "name": "ray/validate-module",
    "description":"Validation module for Ray.Di",
    "keywords":[
        "validation",
        "Ray.Di"
    ],
    "require": {
        "ray/di": "^2.0",
        "nikic/php-parser": "^1.3"
    },
    "license": "MIT",
    "autoload":{
        "psr-4":{
            "Ray\\Validation\\": "src/"
        },
        "exclude-from-classmap": ["tests/Fake"]
    },
    "autoload-dev":{
        "psr-4":{
            "Ray\\Validation\\": ["tests/", "tests/Fake"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
